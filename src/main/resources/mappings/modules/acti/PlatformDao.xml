<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.uwca.operation.modules.activity.dao.PlatformDao">
    
	<select id="findAllPlatform" resultType="Platform">
		SELECT 
			id,
			platformid,
			version 
		FROM appconfig 
		WHERE isdelete = 0 order by version asc
	</select>
	
</mapper>